{
  "name": "pragma-dsp",
  "version": "0.1.0",
  "description": "Useful DSP primitives for TypeScript.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./xform": {
      "types": "./dist/xform/index.d.ts",
      "import": "./dist/xform/index.mjs",
      "require": "./dist/xform/index.cjs"
    },
    "./xform/fourier": {
      "types": "./dist/xform/fourier.d.ts",
      "import": "./dist/xform/fourier.mjs",
      "require": "./dist/xform/fourier.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.cjs"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "import": "./dist/effect/index.mjs",
      "require": "./dist/effect/index.cjs"
    },
    "./math/complex": {
      "types": "./dist/math/index.d.ts",
      "import": "./dist/math/index.mjs",
      "require": "./dist/math/index.cjs"
    },
    "./fluent": {
      "types": "./dist/fluent/index.d.ts",
      "import": "./dist/fluent/index.mjs",
      "require": "./dist/fluent/index.cjs"
    },
    "./xform/fourier-fluent": {
      "types": "./dist/xform/fourier-fluent.d.ts",
      "import": "./dist/xform/fourier-fluent.mjs",
      "require": "./dist/xform/fourier-fluent.cjs"
    }
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "effect": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:reallife": "vitest run test/reallife/",
    "bench": "node --import tsx bench/run.ts",
    "gen:fixtures": "uv run --directory scripts python gen_fixtures.py --overwrite",
    "gen:refs": "uv run --directory scripts python gen_reallife_refs.py --out ../test/reallife/references/"
  },
  "devDependencies": {
    "effect": "^3.0.0",
    "mitata": "^1.0.0",
    "tsdown": "^0.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
